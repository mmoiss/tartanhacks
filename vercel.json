{
  "builds": [
    {
      "src": "front-end/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "api/main.py" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "front-end/$1" }
  ]
}
